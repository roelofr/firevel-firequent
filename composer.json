{
    "name": "firevel/firequent",
    "description": "Limited implementation of Eloquent based on Firestore.",
    "keywords": ["laravel", "eloquent", "firestore", "app engine", "google cloud", "firevel", "firequent", "serverless"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Slowik",
            "email": "michael.slowik@firevel.com"
        }
    ],
    "require": {
        "grpc/grpc": "^1.30.0",
        "google/protobuf": "^v3.13.0",
        "google/cloud-firestore": "^1.14.1",
        "illuminate/contracts": "^5.8 || ^6.0 || ^7.0",
        "illuminate/database": "^5.8 || ^6.0 || ^7.0",
        "illuminate/pagination": "^5.8 || ^6.0 || ^7.0",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8 || ^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Firevel\\Firequent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Firevel\\Firequent\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Firevel\\Firequent\\FirequentServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit"
        ]
    }
}
